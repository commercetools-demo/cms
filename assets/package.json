{
  "name": "@commercetools-demo/cms-asset",
  "private": false,
  "version": "0.5.2",
  "type": "module",
  "files": [
    "dist/**/*",
    "README.md",
    "package.json",
    "index.html"
  ],
  "homepage": "https://github.com/commercetools-demo/cms",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc && yarn build:types",
    "build:types": "dts-bundle-generator --out-file dist/index.d.ts dist/main.ts",
    "preview": "vite preview",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "echo 'No tests for now'",
    "test:watch": "echo 'No tests for now'"
  },
  "devDependencies": {
    "@types/google.maps": "^3.58.1",
    "dts-bundle-generator": "^9.5.1",
    "prettier": "^3.5.3",
    "prettier-plugin-embed": "^0.5.0",
    "typescript": "^5.2.2",
    "vite": "^6.2.5"
  },
  "dependencies": {
    "@commercetools/platform-sdk": "^4.0.0",
    "@commercetools/sdk-client-v2": "^2.0.0",
    "@reduxjs/toolkit": "^2.6.0",
    "@types/sortablejs": "^1.15.8",
    "@types/uuid": "^10.0.0",
    "dotenv": "^16.0.3",
    "lit": "^3.2.1",
    "lit-redux-watch": "^0.3.8",
    "playground-elements": "^0.18.0",
    "redux": "^5.0.1",
    "redux-thunk": "^3.1.0",
    "sortablejs": "^1.15.6",
    "uuid": "^11.1.0"
  },
  "packageManager": "yarn@1.22.22"
}
