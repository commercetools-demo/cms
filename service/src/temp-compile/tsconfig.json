{
  "compilerOptions": {
    "target": "ES2015",
    "module": "ESNext",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "skipLibCheck": true,
    "isolatedModules": true,
    "baseUrl": ".",
    "paths": {
      "lit": [
        "../../node_modules/lit"
      ],
      "lit/*": [
        "../../node_modules/lit/*"
      ]
    }
  }
}